{
  "name": "gijitron",
  "version": "0.3.3",
  "description": "gijitron is an editor to take the minutes of a meeting.",
  "main": "app/main.js",
  "scripts": {
    "test": "gulp comp && node test/test.js",
    "lint": "gulp lint",
    "start": "gulp start",
    "gulp": "gulp",
    "pack": "gulp dist && npm run pack:win32 && npm run pack:darwin && npm run pack:linux",
    "pack:win32": "electron-packager _dist gijitron --platform=win32 --arch=x64 --asar",
    "pack:darwin": "electron-packager _dist gijitron --platform=darwin --arch=x64 --asar",
    "pack:linux": "electron-packager _dist gijitron --platform=linux --arch=x64 --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamofu/gijitron.git"
  },
  "author": "shamofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shamofu/gijitron/issues"
  },
  "homepage": "https://github.com/shamofu/gijitron#readme",
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "caniuse-api": "^2.0.0",
    "del": "^3.0.0",
    "draft-js": "^0.10.4",
    "electron": "^1.7.9",
    "electron-connect": "^0.6.2",
    "electron-packager": "^9.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-useref": "^3.1.2",
    "material-ui": "^0.19.4",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "rcedit": "^0.9.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-tap-event-plugin": "^3.0.2",
    "slash": "^1.0.0",
    "spectron": "^3.7.2",
    "uglify-js": "^3.1.5",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0"
  }
}
